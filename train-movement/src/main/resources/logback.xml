<configuration>
  <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%date{ISO8601} %-5level %-30logger{0} - %msg%n</pattern>
    </encoder>
  </appender>

  <root level="warn">
    <appender-ref ref="console" />
  </root>

  <logger name="net.eatcode" level="debug" />
  
  <logger name="com.hazelcast.client" level="warn" />
</configuration>
